{
	"manifest_version": 2,

	"name": "U-short",
    "version": "1.0",
	"description": "U-short allows user to easily shorten their long URLs!",
	

	"icons": {
		"16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
	},

	"browser_action": {
		"default_icon": "images/logo.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"tabs",
		"https://ur2.pl/"
	]
}
